[GetCapabilities]
title = <http://purl.org/dc/terms/title>
abstract = <http://purl.org/dc/terms/abstract>
keywords = <http://purl.org/dc/terms/subject>

[Geometry]
geometryPredicate = DELETE-ME!!<http://www.opengis.net/ont/geosparql/1.0#asWKT>
geometryVariable = asWKT
geometryClass = <http://www.opengis.net/ont/geosparql/1.0#Geometry>
predicatesContainer = <http://www.opengis.net/ont/geosparql/1.0#Feature>
featureConnector = <http://www.opengis.net/ont/geosparql/1.0#hasGeometry>
wktLiteral = <http://www.opengis.net/ont/sf#wktLiteral>

## Don't touch the [SystemDefaults] variables, unless you really know what you're doing.
[SystemDefaults]
serviceName = lod4wfs
xsdNameSpace = http://www.w3.org/2001/XMLSchema#
defaultLiteral = <http://www.w3.org/2001/XMLSchema#string>
stringLiteral = <http://www.w3.org/2001/XMLSchema#string>
integerLiteral = <http://www.w3.org/2001/XMLSchema#integer>
decimalLiteral =<http://www.w3.org/2001/XMLSchema#decimal>
longLiteral =<http://www.w3.org/2001/XMLSchema#long>
dateLiteral =<http://www.w3.org/2001/XMLSchema#date>
dateLiteral =<http://www.w3.org/2001/XMLSchema#byte>
floatLiteral =<http://www.w3.org/2001/XMLSchema#float>
RDFNamespace = <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
defaultCRS = EPSG:4326
fdaFeaturesNameSpace = http://sparql.lod4wfs.de/
solrFeaturesNameSpace = http://ulb.solr4wfs.de/
sdaPrefix=sda
fdaPrefix=fda
solrPrefix=solr
sdaEnabled = false
fdaEnabled = true
solrEnabled = true

[Server]
defaultPort = 8088
SPARQLEndpointURL = http://giv-lodum.uni-muenster.de:8081/parliament/sparql
SPARQLDirectory = features/

[WebInterface]
PreviewLimit = 10


